{
  "name": "facebook-web-sdk-promise",
  "version": "1.0.0",
  "description": "Load Facebook's SDK and return a Promise. If it's already loading it won't try to load it again.",
  "main": "dist/index.es5.js",
  "jsnext:main": "index.js",
  "scripts": {
    "build": "rollup-babel-lib-bundler --lib-name 'index.es5' --format cjs --dest ./dist ./index.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "console-class": "^2.0.0",
    "tiny-load-script": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015-loose-rollup": "^7.0.0",
    "rollup-babel-lib-bundler": "^2.3.6"
  },
  "babel": {
    "presets": [
      "es2015-loose-rollup"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uch71109/facebook-web-sdk-promise.git"
  },
  "bugs": {
    "url": "https://github.com/uch71109/facebook-web-sdk-promise/issues"
  },
  "homepage": "https://github.com/uch71109/facebook-web-sdk-promise#readme"
}
